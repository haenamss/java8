package hnm;

public class ProductExH {

	public static void main(String[] args) {
		ProductH pro = new ProductH();
		pro.setPid("pkm01");
		pro.setPname("포켓몬빵");
		pro.setAmount(3);
		pro.setPrice(30000);
		pro.setImg("이미지요..?");		
		pro.prinProduct();
		
		System.out.println("--------------------------------------------");		
		System.out.print("판매 금액 : "+pro.calcMoney()+"\t");
		System.out.print("제품 등급 : "+pro.calcRank());
		
		
			
		

	}

}
