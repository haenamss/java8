package hnm;

import java.util.Scanner;

public class HnEx {
	public static void main(String[] args) {
		Hn hn = new Hn();
		Scanner sc = new Scanner(System.in);
		
		System.out.println("제품 코드 : ");
		hn.setPid(sc.nextLine());
		System.out.println("제품명 : ");
		hn.setPname(sc.nextLine());
		System.out.println("제품 수량 : ");
		hn.setAmount(sc.nextInt());
		System.out.println("제품가격 : ");
		hn.setPrice(sc.nextInt());
		System.out.println("제품 이미지 :");
		hn.setImg(sc.nextLine());
		
		System.out.print(hn.getPid()+hn.getPname()+hn.getAmount()+hn.getPrice()+hn.getImg());
		
		
		
		
		
		
		
		
	
	
		}
		
		
	
		
	}


