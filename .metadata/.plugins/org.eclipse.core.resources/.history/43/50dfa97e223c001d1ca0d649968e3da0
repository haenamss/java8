package hnm;

import java.util.Scanner;

public class HnEx {
	public static void main(String[] args) {		
		
		Hn hn = new Hn();		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("제품 코드 : ");
		hn.setPid(sc.next());
		System.out.println("제품명 : ");
		hn.setPname(sc.next());
		System.out.println("제품 수량 : ");
		hn.setAmount(sc.nextInt());
		System.out.println("제품가격 : ");
		hn.setPrice(sc.nextInt());
		System.out.println("제품 이미지 :");
		hn.setImg(sc.next());
		
		
		sc.close();
		System.out.println("--------------------------");
		System.out.println("제품 코드 : "+hn.getPid());
		System.out.println("제품명 : "+hn.getPname());
		System.out.println("제품 수량 : "+hn.getAmount());
		System.out.println("제품 가격 : "+hn.getPrice());
		System.out.println("제품 이미지 : "+hn.getImg());
		System.out.println("제품 판매 가격 : "+hn.calcMoney());
		}
		
	
		}
	
		
	


