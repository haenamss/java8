package sec3;

import java.util.Scanner;

public class hn {

	public static void main(String[] args) {	
		 int value = 0;
		 Scanner scan = new Scanner(System.in);
		 do{
			 value = scan.nextInt();
			 System.out.println("입력한 값 : "+value);
		 }while(value>=10){
			 System.out.println("시스템 종료");
			 
		 }
			 
		 
		 
}
}
