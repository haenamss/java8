package sec1;

public class MemberEx {

	public static void main(String[] args) {
		
		//클래스명 인스턴스명(소문자로 자유롭게★) = new 생성자함수();
		Member mem1 = new Member();
		mem1.setId("lhn");
		mem1.setPassword("lhn1004");
		mem1.setName("이해나");
		mem1.setTel("010-1234-5678");
		
		System.out.println("아이디 : "+mem1.getId());
		System.out.println("비밀번호 : "+mem1.getPassword());
		System.out.println("이름 : "+mem1.getName());
		System.out.println("전화번호 :"+mem1.getTel());
		System.out.println("--------------------");
		
		Member mem2 = new Member();
		mem2
		
		
	}

}
