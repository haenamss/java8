package sec2;

public class Student {
	String name;
	int kor;
	int eng;
	int mat;	
	
	public void setMat(int mat) {
		this.mat = mat;
	}
	public Student(String name, int kor, int eng , int mat){
		this.name = name;
		this.kor = kor;
		this.eng = eng;
		this.mat = mat;
	}
	
	public int calcTot(){
		int tot = kor+eng+mat;
		return tot;
	}
	public double calcAvg(){
		double avg = (kor+eng+mat)/3;
		return avg;
		
		
		
		
	}
		
	
		
}

