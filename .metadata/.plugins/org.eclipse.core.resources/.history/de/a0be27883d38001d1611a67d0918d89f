package hnm;

public class Member {
	//public은 현재 프로젝트 내에 다른 모든 파일들이 접근 가능
	//default 접근 제한자를 생략한 경우 // 같은 패키지 내에서 접근 가능
	//private 는 현재 클래스에서만 접근 가능
	
	private String id;
	private String password;
	private String name;
	private String address;
	private String tel;
	private String reg_date;

	
	public Member(){}

	


	
	
}


