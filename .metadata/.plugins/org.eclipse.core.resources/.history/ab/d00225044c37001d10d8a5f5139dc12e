package sec1;

public class hnm {

	public static void main(String[] args) {
		String str1 = new String("Hello world");
		String str2 = new String("Hello world");
		
		if(str1==str2);{
			System.out.println("str1과 str2는 같은 래퍼런스 입니다.");
		}else{
			System.out.println("str1과 str2는 다른 래퍼런스입니다.");
		}
		

	}

}
