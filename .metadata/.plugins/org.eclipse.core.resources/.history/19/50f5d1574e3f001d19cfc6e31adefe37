package hnm;

public class MemverServiceEx {

	public static void main(String[] args) {
		MemverService mem = new MemverService();
		boolean result = mem.login("lee" , "12345");
		if(result){
			System.out.println("로그인 되었습니다.");
			mem.logout("hong");
		}else{
			System.out.println("id 또는 password가 올바르지 않습니다.");
		}

	}

}
