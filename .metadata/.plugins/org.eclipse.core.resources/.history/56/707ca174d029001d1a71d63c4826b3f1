package sec1;

public class Var03 {

	public static void main(String[] args) {
		//정수
		boolean a=true;
		byte b=20, c=127;	// 1byte = 8bit 
		short d=6450;	// 2byte = 16bit
		int e=650000;	// 4byte = 32bit
		long f=650000000;	// 8byte = 64bit
		//실수
		float g=3.14f;	// 4byte = 32bit
		double h=3.14;	// 8byte = 64bit
		//문자/문자열
		char i='k';
		String j="lee";
		
		

	}

}
