package hnm;

import java.util.Scanner;

public class HnEx {
	public static void main(String[] args) {		
		Scanner sc = new Scanner(System.in);
		Hn hn = new Hn();		
		
		System.out.println("제품 코드 : ");
		hn.setPid(sc.next());
		System.out.println("제품명 : ");
		hn.setPname(sc.next());
		System.out.println("제품 수량 : ");
		hn.setAmount(sc.nextInt());
		System.out.println("제품가격 : ");
		hn.setPrice(sc.nextInt());
		System.out.println("제품 이미지 :");
		hn.setImg(sc.next());
		
		
		sc.close();

		System.out.println("제품 판매 가격 : "+hn.calcMoney(0));
		}
		
	
		}
	
		
	


