package hnm;

public class Printer {
	static public void println(int a){
		System.out.println(a);
	}
	static public void println(boolean a){
		System.out.println(a);
	}
	static public void println(double a){
		System.out.println(a);
	}
	static public void println(String a){
		System.out.println(a);
	}
		
	}

	
	
