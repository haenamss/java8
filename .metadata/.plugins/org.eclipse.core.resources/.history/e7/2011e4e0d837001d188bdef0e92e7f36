package sec2;

public class hn {

	public static void main(String[] args) {
		int max = 0;	//최대값
		int min = 5; //최소값  ( 값중에 0보다 작은 숫자가 없기 때문에 최소값 이상의 숫자를 넣어야 함)
		int[]array = {1,5,3,8,2};
		for(int a=0;a<array.length;a++){
			if(max<array[a]){
				max=array[a];
			}
			
		}System.out.println("max : "+max);
		
		
	}

}
