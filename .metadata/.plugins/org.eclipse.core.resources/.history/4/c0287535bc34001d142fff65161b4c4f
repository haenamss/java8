package sec4;

public class Ex4 {

	public static void main(String[] args) {
		
		int a=(int)(Math.random()*6)+1;
		int b=(int)(Math.random()*6)+1;
		
		while (a>0 && b>0){
			if(a+b==5){
				System.out.println("("+a+", "+b+")");
				break;
			}else if(a+b!=5){
				System.out.println("("+a+", "+b+")");
				
			}
		}
	}

}
